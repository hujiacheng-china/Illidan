<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_zoom_list"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.hujiacheng.custom.zoomlist.ZoomListActivity">

    <include layout="@layout/layout_title"/>

    <com.hujiacheng.custom.zoomlist.ZoomListView
        tools:listheader="@layout/layout_zoomlist_header"
        android:id="@+id/zoom_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </com.hujiacheng.custom.zoomlist.ZoomListView>

</LinearLayout>
